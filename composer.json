{
    "name": "fusic/apollon",
    "description": "CakePHP Apollon",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "validation"],
    "homepage": "https://github.com/fusic/Apollon",
    "license": "MIT",
    "authors": [
        {
            "name": "Fusic",
            "role": "Author"
        }
    ],
    "support": {
        "source": "https://github.com/fusic/Apollon"
    },
    "require": {
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Apollon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apollon\\Test\\": "tests",
            "Apollon\\Test\\App\\": "tests/test_app/App"
        }
    },
    "suggest": {
    },
    "extra": {
    }
}
